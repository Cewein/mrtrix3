mkdir -p ../tmp/dwibiasnormmask && mrconvert BIDS/sub-01/dwi/sub-01_dwi.nii.gz -fslgrad BIDS/sub-01/dwi/sub-01_dwi.bvec BIDS/sub-01/dwi/sub-01_dwi.bval tmp-sub-01_dwi.mif -force && dwibiasnormmask tmp-sub-01_dwi.mif ../tmp/dwibiasnormmask/default_out.mif -output_mask ../tmp/dwibiasnormmask/default_mask.mif -force
dwibiasnormmask tmp-sub-01_dwi.mif -max_iters 3 ../tmp/dwibiasnormmask/3iters_out.mif -output_mask ../tmp/dwibiasnormmask/3iters_mask.mif -force
dwibiasnormmask tmp-sub-01_dwi.mif -lmax 6,0,0 ../tmp/dwibiasnormmask/lmax600_out.mif -output_mask ../tmp/dwibiasnormmask/lmax600_mask.mif -force

