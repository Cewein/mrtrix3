#!/bin/bash
# Verify operation when a user-specified voxel mask is provided
# Ensure that output matches that generated by a prior software version
fod2fixel -nthreads 1 fod.mif tmp-fod2fixel2 \
-mask mask.mif \
-afd afd.mif \
-peak peak.mif \
-disp disp.mif \
-force

testing_diff_fixel -frac 10e-5 fod2fixel/out2 tmp-fod2fixel2
